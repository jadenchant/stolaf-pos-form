{
  "name": "@st-olaf/pos",
  "version": "0.1.0",
  "type": "module",
  "keywords": [
    "react",
    "vite",
    "typescript",
    "gravity",
    "uikit",
    "tailwindcss",
    "sass",
    "react-pdf",
    "react-signature-canvas",
    "react-router"
  ],
  "description": "Vite template with: typescript, sass and @gravity-ui packages",
  "repository": {
    "type": "git",
    "url": "https://github.com/gravity-ui/gravity-ui-vite-example"
  },
  "engines": {
    "node": ">=18"
  },
  "bugs": {
    "url": "https://github.com/gravity-ui/gravity-ui-vite-example/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:styles": "stylelint '{styles,src}/**/*.scss'",
    "lint:prettier": "prettier --check '**/*.md'",
    "lint": "npm run lint:js && npm run lint:styles && npm run lint:prettier",
    "test": "exit 0"
  },
  "dependencies": {
    "@gravity-ui/icons": "^2.12.0",
    "@gravity-ui/uikit": "^5.31.2",
    "@react-pdf/renderer": "^3.4.5",
    "bem-cn-lite": "^4.1.0",
    "react-router": "^6.29.0",
    "react-router-dom": "^6.29.0",
    "react-signature-canvas": "^1.0.7"
  },
  "devDependencies": {
    "@gravity-ui/browserslist-config": "^4.3.0",
    "@gravity-ui/eslint-config": "^3.3.0",
    "@gravity-ui/prettier-config": "^1.1.0",
    "@gravity-ui/stylelint-config": "^4.0.1",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-signature-canvas": "^1.0.7",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "postcss": "^8.5.1",
    "sass": "^1.84.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "vite-plugin-svgr": "^4.3.0"
  },
  "browserslist": [
    "extends @gravity-ui/browserslist-config"
  ],
  "lint-staged": {
    "*.{scss}": [
      "stylelint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
